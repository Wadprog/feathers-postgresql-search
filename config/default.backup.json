{
  "host": "localhost",
  "port": 3030,
  "public": "../public/",
  "paginate": {
    "default": 10,
    "max": 50
  },
  "postgres-knex": {
    "client": "pg",
    "connection": "postgres://postgres:@localhost:5432/sample-app"
  },
  "postgres": {
    "user": "postgres",
    "db": "sample-app",
    "password": "",
    "options": {
      "port": 5432,
      "dialect": "postgres",
      "sync": {
        "force": true,
        "cascade": true
      },
      "pool": {
        "max": 5,
        "min": 0,
        "acquire": 30000,
        "idle": 10000
      },
      "define": {
        "freezeTableName": true
      },
      "host": "localhost"
    }
  }
}
